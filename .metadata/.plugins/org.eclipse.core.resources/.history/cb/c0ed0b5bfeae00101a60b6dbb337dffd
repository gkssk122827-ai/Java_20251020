package test;

public class Example01 {

	public static void main(String[] args) {
		
		/*
		 * ------출력 예시 -----
		 * 이름 : 본인이름			: name
		 * 나이 : 				: age
		 * 주소 : 				: address
		 * 몸무게 : 				: weight
		 * 
		 * 
		 */

		String name = "이한나";
		System.out.println("이름 :" + name);
		
		int age = 31;
		System.out.println("나이 :" + age);
		
		String address = "서울특별시 강동구 명일동";
		System.out.println("주소 :" + address);
		
		double weight = 52.5;
		System.out.println("몸무게 :" + weight + "Kg");
		
		
		
	}

}
