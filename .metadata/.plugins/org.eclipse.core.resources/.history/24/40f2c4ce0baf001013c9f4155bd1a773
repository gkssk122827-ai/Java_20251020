package test;

public class Sample2 {

	public static void main(String[] args) {
		
		System.out.println("Hello, java");
		System.out.println("Hello, java");
		
		Person P = new Person();
		
		P.name = "이한나";
		P.age = 31;
				
		System.out.println(P.name);
		System.out.println(P.age);
		
		
	}

}
class Person{
	String name;
	int age;
	
}