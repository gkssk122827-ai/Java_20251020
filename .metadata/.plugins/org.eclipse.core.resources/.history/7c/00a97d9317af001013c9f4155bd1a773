package ex02;

public class Calculator {

	public static void main(String[] args) {
		
		int x = 1;
		int y = 2;
		int result = x+y;
		
	    System.out.println("두 수 합은 : " + result + " 입니다.");
	    
	    int korsocre = 79;  
	    int engSocre = 80;  
	    int mathSocre = 99;  
	    
	    int sum = korsocre + engSocre + mathSocre;
	    
	    System.out.println(sum);

	}

}
